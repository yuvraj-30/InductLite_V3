{
  "name": "@inductlite/web",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test e2e/visual-regression.spec.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "run:export-scheduler": "tsx scripts/run-export-scheduler.ts",
    "run:maintenance": "tsx scripts/run-maintenance.ts",
    "test:upstash-e2e": "node ./scripts/run-upstash-e2e.js",
    "test:upstash-e2e:sh": "bash ./scripts/run-upstash-e2e.sh",
    "test:upstash-e2e:ps1": "powershell -File ./scripts/run-upstash-e2e.ps1",
    "vrt:up": "docker compose -f ../../docker-compose.vrt.yml up -d",
    "vrt:down": "docker compose -f ../../docker-compose.vrt.yml down --volumes",
    "vrt:logs": "docker compose -f ../../docker-compose.vrt.yml logs -f",
    "prisma:validate": "prisma validate",
    "prisma:format:check": "prisma format --check",
    "test:e2e:smoke": "playwright test e2e/public-signin.spec.ts e2e/admin-auth.spec.ts e2e/csrf-security.spec.ts --project=chromium --workers=1",
    "test:e2e:full": "playwright test e2e --project=chromium --project=firefox --project=webkit --workers=1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@inductlite/shared": "file:../../packages/shared",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/semantic-conventions": "^1.26.0",
    "@prisma/client": "^5.22.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "archiver": "^7.0.1",
    "argon2": "^0.41.1",
    "date-fns": "^4.1.0",
    "iron-session": "^8.0.4",
    "libphonenumber-js": "^1.12.35",
    "nanoid": "^5.0.9",
    "next": "16.1.5",
    "otplib": "^12.0.1",
    "pg-boss": "^10.1.5",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^24.36.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "resend": "^3.5.0",
    "set-interval-async": "^3.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "dotenv": "^16.4.7",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.49.0",
    "@testcontainers/postgresql": "^11.11.0",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.17.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.5",
    "eslint-plugin-security-guardrails": "file:./eslint-plugin-security",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.16",
    "testcontainers": "^11.11.0",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
